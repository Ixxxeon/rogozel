}\n\n  delete(department: IDepartment) {\n    const modalRef = this.modalService.open(DepartmentDeleteDialogComponent, { size: 'lg', backdrop: 'static' });\n    modalRef.componentInstance.department = department;\n  }\n}\n"]}]}